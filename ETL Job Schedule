USE msdb;
GO

SELECT 
    j.name AS JobName,
    j.enabled AS JobEnabled,
    s.name AS ScheduleName,
    s.enabled AS ScheduleEnabled,
    CASE s.freq_type
        WHEN 1 THEN 'Once'
        WHEN 4 THEN 'Daily'
        WHEN 8 THEN 'Weekly'
        WHEN 16 THEN 'Monthly'
        WHEN 32 THEN 'Monthly relative'
        WHEN 64 THEN 'When SQL Server Agent starts'
        WHEN 128 THEN 'Start whenever CPU idle'
        ELSE 'Other'
    END AS FrequencyType,
    s.freq_interval AS FrequencyInterval,
    s.freq_subday_type AS SubDayType,
    s.freq_subday_interval AS SubDayInterval,
    s.freq_relative_interval AS RelativeInterval,
    s.freq_recurrence_factor AS RecurrenceFactor,
    STUFF(
        RIGHT('000000' + CAST(s.active_start_time AS VARCHAR(6)), 6),
        3,0,':'
    ) AS ActiveStartTimeHHMMSS,
    CAST(s.active_start_date AS VARCHAR(8)) AS ActiveStartDate
FROM 
    msdb.dbo.sysjobs j
    INNER JOIN msdb.dbo.sysjobschedules js ON j.job_id = js.job_id
    INNER JOIN msdb.dbo.sysschedules s ON js.schedule_id = s.schedule_id
WHERE 
    j.enabled = 1
    AND j.name LIKE '%DXRA%'
ORDER BY 
    j.name, s.active_start_time;
