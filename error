            ON pw.ConfigurationName = 'ftpPassword'
        INNER JOIN dbo.ETL_Configuration un
            ON un.ConfigurationName = 'ftpUserName'
        WHERE sc.ConfigurationName IN ('SourceCode','ClientID')
          AND pw.ConfigurationEncrypted IS NOT NULL;
